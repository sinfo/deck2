{
    "delete": {
        "tags": [
            "teams", "meetings"
        ],
        "summary": "Removes a  meeting from a team",
        "operationId": "deleteTeamMeeting",
        "consumes": [
            "application/json"
        ],
        "produces": [
            "application/json"
        ],
        "security": [
            {
              "Bearer": []
            }
          ],
        "parameters": [
            {
                "in": "path",
                "name": "id",
                "description": "ID of the team",
                "required": true,
				"type": "string"
            },
            {
                "in":"path",
                "name": "meetingID",
                "description":"ID of the meeting",
                "required":true,
                "type":"string"
            }
        ],
        "responses": {
            "200": {
                "description": "Removed meeting",
                "schema": {
                    "$ref": "./models/meeting.json"
                }
            },
            "401": {
                "description": "Unauthorized"
            },
            "404":{
                "description":"Couldn't find team or meeting, or meeting not on team"
            },
            "417": {
                "description": "Couldn't find team"
            }
        }
    }
}