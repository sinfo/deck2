{
    "post": {
        "tags": [
            "speakers"
        ],
        "summary": "Update speaker's company public image by ID",
        "operationId": "updateSpeakerCompanyImage",
        "consumes": [
            "multipart/form-data"
        ],
        "produces": [
            "application/json"
        ],
        "security": [
            {
              "Bearer": []
            }
          ],
        "parameters": [
            {
                "in": "path",
                "name": "id",
                "description": "ID of the speaker",
                "required": true,
				"type": "string"
            },
            {
                "in": "formData",
                "name": "image",
                "type": "file",
                "description": "Logo of speaker's company"
            }
        ],
        "responses": {
            "200": {
                "description": "Speaker with the updated data",
                "schema": {
                    "$ref": "./models/speaker.json"
                }
            },
            "417": {
                "description": "Unable to perform operation"
            },
            "404": {
                "description": "Speaker not found"
            },
            "401": {
                "description": "Unauthorized"
            },
            "400": {
                "description": "Invalid image data"
            }
        }
    }
}