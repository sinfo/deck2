{
    "put": {
        "tags": [
            "members", "contacts"
        ],
        "summary": "Updates the member's contact",
        "operationId": "updateMemberContact",
        "consumes": [
            "application/json"
        ],
        "produces": [
            "application/json"
        ],
        "parameters": [
            {
                "in": "path",
                "name": "id",
                "description": "ID of the member",
                "required": true,
				"type": "string"
            },
            {
                "in": "body",
                "name": "payload",
                "description": "New contact data",
                "required": true,
                "schema": {
                    "type": "object",
                    "required": [
                        "id"
                    ],
                    "properties": {
                        "id": {
                            "type": "string"
                        }
                    }
                }
            }
        ],
        "responses": {
            "200": {
                "description": "Updated member",
                "schema": {
                    "$ref": "./models/member.json"
                }
            },
            "400": {
                "description": "Invalid payload, or couldn't update member"
            },
            "404": {
                "description": "Couldn't find member or contact"
            }
        }
    }
}