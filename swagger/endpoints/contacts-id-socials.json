{
    "put": {
        "tags": [
            "contacts"
        ],
        "summary": "Updates a contact's socials",
        "operationId": "updateSocials",
        "consumes": [
            "application/json"
        ],
        "produces": [
            "application/json"
        ],
        "security": [
            {
              "Bearer": []
            }
          ],
        "parameters": [
            {
                "in": "path",
                "name": "id",
                "description": "ID of the member",
                "required": true,
                "type": "string"
            },
            {
                "in": "body",
                "name": "payload",
                "description": "Updated socials list",
                "required": true,
                "schema": {
                    "type": "object",
                    "required": [
                        "facebook",
                        "skype",
                        "github",
                        "twitter",
                        "linkedin"
                    ],
                    "properties": {
                        "facebook": {
                            "type": "string"
                        },
                        "skype": {
                            "type": "string"
                        },
                        "github": {
                            "type": "string"
                        },
                        "twitter":{
                            "type":"string"
                        },
                        "linkedin":{
                            "type":"string"
                        }
                    }
                }
            }
        ],
        "responses": {
            "200": {
                "description": "Updated contact.",
                "schema": {
                    "$ref": "./models/contact.json"
                }
            },
            "401": {
                "description": "Unauthorized"
            },
            "400": {
                "description": "Invalid input or couldn't updated the contact."
            },
            "404": {
                "description": "Could not find contact"
            }
        }
    }
}