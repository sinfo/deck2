{
    "get": {
        "tags": [
            "billings"
        ],
        "summary": "Get all billings. If role is lower than coordinator, only visible billings will be returned",
        "operationId": "getBillings",
        "consumes": [
            "application/json"
        ],
        "produces": [
            "application/json"
        ],
        "security": [
            {
              "Bearer": []
            }
          ],
        "parameters": [
            {
                "in": "query",
                "name": "after",
                "description": "Billings emited after date",
                "type": "string",
                "format":"date-time"
            },
            {
                "in": "query",
                "name": "before",
                "description": "Billings emited before date",
                "type": "string",
                "format":"date-time"
            },
            {
                "in": "query",
                "name": "valueGreaterThan",
                "description": "Billings with value greater than this value",
                "type": "string"
            },
            {
                "in": "query",
                "name": "valueLessThan",
                "description": "Billings with value lower than this value",
                "type": "string"
            },
            {
                "in": "query",
                "name": "event",
                "description": "Billings from this event",
                "type": "string"
            },
            {
                "in": "query",
                "name": "company",
                "description": "Billings from this company",
                "type": "string"
            }

        ],
        "responses": {
            "200": {
                "description": "Billings filtered by the query",
                "schema": {
                    "type": "array",
                    "items": {
                        "$ref": "./models/billing.json"
                    }
                }
            },
            "401": {
                "description": "Unauthorized"
            },
            "417": {
                "description": "Unable to get billings"
            }
        }
    },
    "post": {
        "tags": [
            "billings"
        ],
        "summary": "Creates a new billing",
        "operationId": "createBilling",
        "consumes": [
            "application/json"
        ],
        "produces": [
            "application/json"
        ],
        "security": [
            {
              "Bearer": []
            }
          ],
        "parameters": [
            {
                "in": "body",
                "name": "payload",
                "description": "Information needed to create the new company.",
                "required": true,
                "schema": {
                    "type": "object",
                    "required": [
                        "status", "event", "value", "invoiceNumber", "emission", "notes"
                    ],
                    "properties": {
                        "status": {
                            "type": "object",
                            "required":["proForma","paid", "receipt", "invoice"],
                            "properties":{
                                "proForma":{
                                    "type":"boolean"
                                },
                                "paid":{
                                    "type":"boolean"
                                },
                                "receipt":{
                                    "type":"boolean"
                                },
                                "invoice":{
                                    "type":"boolean"
                                }
                            }
                        },
                        "event": {
                            "type": "integer"
                        },
                        "value": {
                            "type": "integer"
                        },
                        "invoiceNumber":{
                            "type": "string"
                        },
                        "emission":{
                            "type":"string",
                            "format":"date-time"
                        },
                        "notes":{
                            "type":"string"
                        }
                    }
                }
            }

        ],
        "responses": {
            "200": {
                "description": "Created billing",
                "schema": {
                    "$ref": "./models/billing.json"
                }
            },
            "401": {
                "description": "Unauthorized"
            },
            "403":{
                "description": "Valid authorization, but not enough credentials"
            },
            "404": {
                "description": "Couldn't find created billing"
            }
        }
    }
}