{
    "delete": {
        "tags": [
            "events", "items"
        ],
        "summary": "Removes item from the current event's packages",
        "operationId": "removeEventItem",
        "consumes": [
            "application/json"
        ],
        "produces": [
            "application/json"
        ],
        "parameters": [
            {
                "in": "path",
                "name": "id",
                "description": "ID of the item",
                "required": true,
                "type": "string"
            }
        ],
        "responses": {
            "200": {
                "description": "Event with the removed item",
                "schema": {
                    "$ref": "./models/event.json"
                }
            },
            "404": {
                "description": "Event not found"
            },
            "417": {
                "description": "Unable to remove item from event"
            }
        }
    }
}